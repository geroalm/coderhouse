      <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Categor√≠a</th>
        <th scope="col">Precio</th>
        <th scope="col">Cantidad</th>
      </tr>
    </thead>
    <tbody>
      {{#each items}}
      <tr>
        <td>{{this.product.title}}</td>
        <td>{{this.product.category}}</td>
        <td>{{this.product.price}}</td>
        <td>{{this.quantity}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
